{
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "216659404274.dkr.ecr.ap-southeast-2.amazonaws.com/dockerzon-nginx:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": ["express:express"],
      "volumesFrom": [
        {
          "sourceContainer": "express"
        }
      ],
      "cpu": 256,
      "memory": 100
    },
    {
      "name": "express",
      "image": "216659404274.dkr.ecr.ap-southeast-2.amazonaws.com/dockerzon-express:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "dockerzon",
          "awslogs-multiline-pattern": "^DOCKERZON::",
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/dockerzon/dev"
        }
      },
      "cpu": 768,
      "memory": 300,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://dockerzon:52230365@dockerzon-production.c3mu4bphl5ah.ap-southeast-2.rds.amazonaws.com:5432/dockerzon?encoding=utf8&pool=40&timeout=5000"
        }
      ]
    }
  ],
  "family": "web"
}
